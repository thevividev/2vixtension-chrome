{
  "manifest_version": 3,
  "name": "2VIxtension",
  "description": "Notifications quand TheVivi est en live",
  "version": "1.0.1",
  "icons": {
    "16": "/assets/16.png",
    "48": "/assets/48.png",
    "128": "/assets/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "wss://bretheskevin.fr/",
    "ws://bretheskevin.fr:4001/"
  ]
}